<div class="container">
    <% @user_listings.each do |listings| %>
        <div class="listing row">
            <h3>Listing</h3>
            <div class="span5 items-offered">
              <h3>Items Offered:</h3>
              <% listings.items_offered.each do |item| %>
                     <div class="item-div" id="<%= item.id%>">
                        <img class="item-img" src="<%= item.image_url %>">
                     </div>
              <% end %>
            </div>
            <div class="span5 items-wanted">
              <h3>Items Wanted:</h3>
              <% listings.items_wanted.each do |item| %>
                     <div class="item-div" id="<%= item.id%>">
                        <img class="item-img" src="<%= item.image_url %>">
                     </div>
              <% end %>
            </div>
        </div>
    <% end %>
</div>

